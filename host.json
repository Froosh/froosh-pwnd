{
    "version": "2.0",
    "extensions": {
        "http": {
            "routePrefix": "api",
            "dynamicThrottlesEnabled": true,
            "hsts": {
                "isEnabled": true,
                "maxAge": "10",
                "preload": true
            }
        }
    },
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[3.3.0, 4.0.0)"
    },
    "healthMonitor": {
        "enabled": true
    },
    "logging": {
        "fileLoggingMode": "always",
        "logLevel": {
            "default": "Trace"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    }
}
